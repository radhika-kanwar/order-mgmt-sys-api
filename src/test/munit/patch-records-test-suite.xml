					<munit-tools:with-attribute whereValue="990188bb-e216-4281-92bb-466a16fd2d2b" attributeName="doc:id" />
				</munit-tools:with-attributes>
				<munit-tools:then-return >
					<munit-tools:variables >
						<munit-tools:variable key="dbOutput" value='#[output application/json --- {"affectedRows": 1}]' mediaType="application/json" encoding="UTF-8" />
