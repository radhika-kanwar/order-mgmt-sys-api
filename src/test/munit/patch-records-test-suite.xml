				<munit:attributes value='#[{&#10;	"headers": {&#10;		"table-name": "customers",&#10;		"primary-key": "customer_id"&#10;	},&#10;	"uriParams": {&#10;		"id": "1"&#10;	}&#10;}]' />
			
</munit:set-event>
